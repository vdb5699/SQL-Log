SELECT TOP 10
CAST(ASSETMANAGEMENT.ASSETLOG."ASSETLOGKEY" AS VARCHAR) + '_ASSETLOG' AS "ID (PK)",
ASSETMANAGEMENT.ASSETLOG."COMMENTS",
ASSETMANAGEMENT.ASSETLOG."COMMENTS_SEARCH",
ASSETMANAGEMENT.ASSETLOG."MODDTTM",
ASSETMANAGEMENT.ASSETLOG."ADDDTTM",
'ASSETLOG' AS "TYPE"
FROM ASSETMANAGEMENT.ASSETLOG
